const int buttonPin = 2;
const int ledPin = 13;
Servo motorone;
Servo motortwo;
Servo motorthree;

int buttonState = 0;

void setup() {
  pinMode(ledPin, OUTPUT);
  pinMode(buttonPin, INPUT);
  my.servo.attach(9);
}
##first scene, putting first prop in place will activate led
void loop() {
  buttonState = digitalRead(buttonPin);

  if (buttonState == HIGH) {
    digitalWrite(ledPin, HIGH);
  } else {
    digitalWrite(ledPin, LOW);
  }
}
